<?xml version="1.0"?>
<!-- Magento compatible wrapper for Apache log4php logging framework -->
<config>
    <modules>
        <Nmmlm_Log>
            <version>0.4.0</version>
        </Nmmlm_Log>
    </modules>
    <adminhtml>
        <translate>
            <modules>
                <Nmmlm_Log>
                    <files>
                        <default>Nmmlm_Log.csv</default>
                    </files>
                </Nmmlm_Log>
            </modules>
        </translate>
    </adminhtml>
    <default>
        <dev>
            <log>
                <!-- Relative path to default logging configuration -->
                <nmmlm_log4php_config_file>app/etc/nmmlm/log/log4php.cfg.xml</nmmlm_log4php_config_file>
            </log>
        </dev>
    </default>
</config>