{
  "name": "praxigento/mage_ext_log4php_test",
  "type": "project",
  "license": "MIT",
  "homepage": "https://github.com/praxigento/mage_ext_log4php",
  "description": "Magento project with deployment script and mage_ext_log4php extension.",
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    },
    {
      "type": "vcs",
      "url": "https://github.com/praxigento/mage_ext_log4php"
    }
  ],
  "require": {
    "composer/composer": "1.0.*@alpha",
    "symfony/console": "2.5",
    "magento-hackathon/magento-composer-installer": "*",
    "magento-hackathon/composer-command-integrator": "1.0",
    "magento/core": "1.9.1.1-patch2",
    "praxigento/mage_ext_log4php": "dev-master"
  },
  "extra": {
    "magento-root-dir": "mage",
    "magento-deploystrategy": "symlink",
    "magento-force": "override",
    "auto-append-gitignore": false
  },
  "minimum-stability": "dev"
}
